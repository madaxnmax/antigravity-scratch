{
    "name": "metalflow-app",
    "version": "1.0.0",
    "scripts": {
        "start": "node server/index.js",
        "build": "npm install --prefix client && npm run build --prefix client && npm install --prefix server",
        "dev": "concurrently \"npm run dev --prefix client\" \"node server/index.js\""
    },
    "dependencies": {
        "concurrently": "^8.2.2"
    },
    "engines": {
        "node": "18.x"
    }
}